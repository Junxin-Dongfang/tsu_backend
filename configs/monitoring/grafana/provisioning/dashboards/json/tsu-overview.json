    {
      "datasource": "Prometheus",
      "description": "自动筛选近 5 分钟内最慢的 HTTP 路由，单位毫秒。",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 200
              }
            ]
          },
          "unit": "ms"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "qps"
            },
            "properties": [
              {
                "id": "unit",
                "value": "reqps"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "id": 7,
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "p95 (ms)"
          }
        ]
      },
      "targets": [
        {
          "expr": "((histogram_quantile(0.95, sum by (le, route)(rate(tsu_http_request_duration_seconds_bucket{service=~\"$service\"}[5m]))) * 1000)) and on(route) (sum by(route)(rate(tsu_http_requests_total{service=~\"$service\"}[5m])) > 0)",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "P95"
        },
        {
          "expr": "((histogram_quantile(0.99, sum by (le, route)(rate(tsu_http_request_duration_seconds_bucket{service=~\"$service\"}[5m]))) * 1000)) and on(route) (sum by(route)(rate(tsu_http_requests_total{service=~\"$service\"}[5m])) > 0)",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "P99"
        },
        {
          "expr": "sum by (route)(rate(tsu_http_requests_total{service=~\"$service\"}[5m]))",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "QPS"
        }
      ],
      "title": "慢路由 (>200ms)",
      "transformations": [
        {
          "id": "joinByField",
          "options": {
            "byField": "route"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "Value": "p95 (ms)",
              "Value #P95": "p95 (ms)",
              "Value #P99": "p99 (ms)",
              "Value #QPS": "qps"
            }
          }
        }
      ],
      "type": "table"
    },
