[
  {
    "id": "tsu-admin-api-rule",
    "match": {
      "url": "http://<.*>/api/admin/<.*>",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    },
    "authenticators": [{ "handler": "cookie_session" }],
    "authorizer": { "handler": "allow" },
    "mutators": [{ "handler": "header" }],
    "upstream": {
      "url": "http://admin:8081",
      "strip_path": "/api/admin"
    },
    "errors": [
      {
        "handler": "redirect",
        "when": {
          "error": [
            "unauthorized"
          ]
        }
      }
    ]
  }
]